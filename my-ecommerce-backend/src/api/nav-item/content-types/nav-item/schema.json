{
  "kind": "collectionType",
  "collectionName": "nav_items",
  "info": { "singularName": "nav-item", "pluralName": "nav-items", "displayName": "Nav Item" },
  "options": { "draftAndPublish": true },
  "attributes": {
    "label": { "type": "string", "required": true },
    "url": { "type": "string", "required": true },
    "order": { "type": "integer", "default": 0 },
    "menu": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu.menu",
      "inversedBy": "items",
      "required": true
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::nav-item.nav-item"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nav-item.nav-item",
      "mappedBy": "parent"
    }
  }
}